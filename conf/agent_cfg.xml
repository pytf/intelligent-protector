<?xml version="1.0" encoding="utf-8" ?>
<root>
    <System>
        <!-- Agent FastCGI listening port -->
        <port value="8091" />
        <!-- Agent log level -->
        <log_level value="1" />
        <!-- Agent log count -->
        <log_count value="3" />
        <!-- Agent log cache threshold -->
        <log_cache_threshold value="30" />
        <!-- Agent access user name -->
        <name value="" />
        <!-- sl value -->
        <sl value="2709023617" />
        <!-- hash value -->
        <hash value="" />
    </System>
    <Monitor>
        <retry_time value="3" />
        <monitor_interval value="30" />
        <rdagent value="1">
            <!--thread count -->
            <thread_count value="100" />
            <!--handler count -->
            <handle_count value="1000" />
            <!--physical memory used in M unit-->
            <pm_size value="100" />
            <!--virtual memory used in M unit-->
            <vm_size value="2000" />
            <!--CPU usage -->
            <cpu_usage value="80" />
            <!--temp directory size in M unit-->
            <tmpfile_size value="20" />
        </rdagent>
        <nginx value="1">
            <!--thread count -->
            <thread_count value="100" />
            <!--handler count -->
            <handle_count value="1000" />
            <!--physical memory used in M unit -->
            <pm_size value="100" />
            <!--virtual memory used in M unit -->
            <vm_size value="2000" />
            <!--CPU usage-->
            <cpu_usage value="80" />
            <!--max nginx log size in M unit-->
            <nginx_log_size value="3" />
            <!--nginx ssl certificate key password -->
            <ssl_key_password value=""/>
        </nginx>
    </Monitor>
    <SNMP>
        <context_name value="trapcontext"/>
        <engine_id value="800063A203000056000000"/>
        <private_password value=""/>
        <private_protocol value="4"/>
        <auth_password value=""/>
        <auth_protocol value="5"/>
        <security_level value="3"/>
        <security_name value="rdadmin"/>
        <security_model value="3"/>
    </SNMP>
    <Upgrade>
        <V100R003C10>
            <version value="all" />
        </V100R003C10>
        <V100R005C00>
            <version value="all" />
        </V100R005C00>
        <V200R001C00>
            <version value="|SPC100|SPC200|SPC201|SPC300" />
        </V200R001C00>
        <V200R001C10>
            <version value="|SPC001|SPC002|SPC100|SPC101|SPC200"/>
        </V200R001C10>
    </Upgrade>
</root>
